  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "(.*)/posts.json": "<rootDir>/__mocks__/assets/posts.json",
      "(.*)/examples.json": "<rootDir>/__mocks__/assets/examples.json",
      "(.*)/categories.json": "<rootDir>/__mocks__/assets/categories.json"
