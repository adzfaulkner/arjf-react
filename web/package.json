    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/setupTests.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
